<body>
<p id="notice"><%= notice %></p>

<h1>Audios</h1>

<table>


  <tbody>
    <% @audios.each do |audio| %>
    <tr class="<%= cycle('list_line_odd', 'list_line_even') %>">
    <td>
      <%#= image_tag(audio.url_audio, class: 'list_image')%>
      <%= soundcloud(audio)%>
        <%= link_to 'Show', audio, class:"btn btn-primary" %>
        <%= link_to 'Edit', edit_audio_path(audio), class:"btn btn-warning"  %>
        <%= link_to 'New Audio', new_audio_path, class: 'btn btn-success' %>
        <%= link_to 'Destroy', audio, class: 'btn btn-danger', method: :delete, data: { confirm: 'Are you sure?' } %>
    </td>



    <% end %>
  </tbody>
</table>



